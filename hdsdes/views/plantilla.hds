{{#if productos}}
    <table>
        <tr>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Foto</th>
        </tr>
        {{#each productos}}
        <tr>
            <td>{{this.nombre}}</td>
            <td>{{this.precio}}</td>
            <td><img src="{{this.foto}}"></td>
        </tr>
        {{/each}}
    </table>
{{else}}
    <h2>No hay productos</h2>
{{/if}}


