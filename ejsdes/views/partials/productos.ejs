<p class="titulo">Productos</h1>
<% if(productos.length > 0){ %>
    <table>
        <tr>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Foto</th>
        </tr>
        <% for(const producto of productos){ %>
        <tr>
            <td><%=producto.nombre%></td>
            <td>$<%=producto.precio%></td>
            <td><img src=<%=producto.foto%>></td>
        </tr>
        <% } %>
    </table>
<% }else { %>
    <p class="aviso">No hay productos</p>
<% } %>
<a href='/'><button>Ingresar producto</button></a>